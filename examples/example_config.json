{
  "input": "examples/example_input.json",
  "output": "outputs/example_output.json",
  "generations": 3,
  "concurrency": 20,
  "models": [
    {
      "name": "gpt-4o",
      "provider": "openai",
      "mode": "sequential"
    },
    {
      "name": "claude-sonnet-4-5-20250929",
      "provider": "anthropic",
      "mode": "batch",
      "api_params": {
        "thinking": {"type": "enabled", "budget_tokens": 10000},
        "max_tokens": 30000
      }
    },
    {
      "name": "gemini-2.5-pro",
      "provider": "gemini",
      "mode": "batch",
      "api_params": {
        "thinking_config": {"thinkingBudget": 10000, "includeThoughts": true},
        "maxOutputTokens": 30000
      }
    },
    {
      "name": "meta-llama/llama-3.3-70b-instruct",
      "provider": "openrouter",
      "mode": "sequential",
      "api_params": {
        "provider": {"order": ["groq"], "allow_fallbacks": false}
      }
    },
    {
      "name": "meta/llama-4-maverick-17b-128e-instruct-maas",
      "provider": "vertexai_oss",
      "mode": "sequential",
      "api_params": {"location": "us-east5"}
    },
    {
      "name": "deepseek-chat",
      "provider": "openai_compatible",
      "mode": "sequential",
      "base_url": "https://api.deepseek.com/v1",
      "api_key_env": "DEEPSEEK_API_KEY"
    }
  ]
}
